lookup kernHorizontalKerninginLatinloo {
  lookupflag 0;
    pos \F \period -130;
    pos \F \comma -150;
    pos \F \period -130;
    pos \F \comma -150;
    pos \L \quoteright -90;
    pos \L \quotedblright -230;
    pos \L \quoteright -90;
    pos \L \quotedblright -230;
    pos \f \quoteright 90;
    pos \f \period -20;
    pos \f \quoteright 90;
    pos \f \period -20;
} kernHorizontalKerninginLatinloo;

