feature kern {

 script latn;
     language dflt ;
      lookup kernHorizontalKerninginLatinloo;
      lookup pos_pair_kernlatn_0;
      lookup pos_pair_kernlatn_2;
} kern;

feature mark {

 script DFLT;
     language dflt ;
      lookup markMarkPositioninglookup1;

 script grek;
     language dflt ;
      lookup markMarkPositioninglookup1;

 script latn;
     language dflt ;
      lookup markMarkPositioninglookup1;
} mark;
